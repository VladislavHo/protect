{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/rates/layout.js", "src/app/rates/page.jsx", "src/app/components/QA/QA.jsx", "src/app/components/Footer/Footer.jsx", "src/app/components/Devices/Devices.jsx", "src/app/components/Buttons/ButtonDefault.jsx", "src/app/components/Buttons/ButtonGreen.jsx", "src/app/components/RatesCards/RatesCardsWrapper.jsx", "src/app/components/AboutServices/AboutServices.jsx", "src/app/components/Safety/Safety.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
